CREATE PROC [dbo].[SP_INSERT_CNAE](
    @Codigo VARCHAR(7),
    @Descrição VARCHAR(50)
)
AS
/*
DOCUMENTAÇÃO
ARQUIVO FONTE.....: SP_INSERT_CNAE.SQL
OBJETIVO..........: INSERIR DADOS NA TABELA CNAE
AUTOR.............: ERICK ALVES
DATA..............: 04/01/2024
EX................: EXEC [dbo].[SP_INSERT_CNAE]  @Codigo, @Descrição
*/
    IF ID_CNAE IS NULL RETURN 1
    BEGIN
        INSERT INTO Cnae(Codigo, Descrição)
            VALUES(@Codigo, @Descrição)
    END
GO