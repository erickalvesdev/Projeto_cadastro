--Criada procedure para update na tabela premiacao
CREATE PROC [dbo].[SP_Up_PREMIACAO](
    @Id INT,
    @IdRanking INT,
    @IdCadastroEmpresa INT,
    @IdTipoPremiacao INT,
    @PremiacaoAtual MONEY,
    @Mes TINYINT,
    @Ano SMALLINT
)
AS
/*
DOCUMENTAÇÃO
ARQUIVO FONTE.....: SP_Up_TIPO_PREMIACAO.SQL
OBJETIVO..........: ATUALIZANDO DADOS NA TABELA TIPO DE PREMIACAO
AUTOR.............: ERICK ALVES
DATA..............: 04/01/2024
EX................: EXEC [SP_Up_TIPO_PREMIACAO] @ID, @IdRanking, @IdCadastroEmpresa, @IdTipoPremiacao, @PremiacaoAtual,
                                                @Mes, @Ano
*/
    IF ID_Premiacao IS NULL RETURN 1
    BEGIN 
		UPDATE Premiacao
			SET PremiacaoAtual = @PremiacaoAtual,
                Mes = @Mes,
                Ano = @Ano

		WHERE   ID = @ID AND
                IdRanking = @IdRanking AND
                IdCadastroEmpresa = @IdCadastroEmpresa AND
                ID_TipoPremiacao = @IdTipoPremiacao
	END